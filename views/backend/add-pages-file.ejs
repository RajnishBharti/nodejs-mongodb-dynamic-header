
<%-include('common/head.ejs')-%>

<title>Page Title</title>
</head>
<body>

    <%-include('common/header.ejs')-%>
 

    <div class="container">
        <div class="row">
            <form class="mt-3" method='post' action="/admin/add-pages">
                <h1 class="mb-3">Add Page</h1>
                
                <div class="mb-3"><input type="text" name="paget_Title" class="form-control" placeholder="Page Title"></div>
                <div class="mb-3"><input type="text" name="page_meta_description" class="form-control"  placeholder="Page meta Description"></div>
                <div class="mb-3"><input type="text" name="page_meta_keyword" class="form-control"  placeholder="Page meta Keyword"></div>
                <div class="mb-3"><input type="text" name="page_heading" class="form-control"  placeholder="input Your Page heading"></div>
                <div class="mb-3"><input type="text" name="page_url" class="form-control"  placeholder="input Page url (linK)"></div>
                <div class="mb-3"><input type="text" name="page_url_text" class="form-control"  placeholder="input Page url text"></div>
                <div class="mb-3"><input type="file" name="page_photo" class="form-control" >
                </div>

                <div class="mb-3"><textarea name="page_details" class="form-control"  rows="10" placeholder="Input Page Details.."></textarea></div>
                <div class="mb-3"><button class="btn btn-success">Submit</button></div>
            </form>
        </div>
    </div>

</body>
</html>